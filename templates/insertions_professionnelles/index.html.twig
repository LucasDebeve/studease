{% extends 'base.html.twig' %}

{% block title %}Stages et Alternances disponibles{% endblock %}

{% block body %}
    <h1>Stages et Alternances disponibles</h1>
    <ul>
        {% for insertion in insertions %}
            <li><a href="{{ path('app_contact_id', {id: insertion.id })}}"><h3>{{ insertion.titre }}</h3> : Durée : {{ insertion.duree }}, Début : {{ insertion.dateDeb }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
